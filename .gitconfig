[merge]
     tool = vimdiff
[core]
     editor = vim
     quotepath = false
[color]
	ui = true
[color "branch"]
     current = yellow reverse
     local = yellow
     remote = green
[color "status"]
     added = yellow
     change = green
     untracked = cyan
[diff]
     tool = vimdiff
[alias]
     lg = log --color --graph --pretty
     df = difftool
     ci = commit
     co = checkout
     st = status
     br = branch
     last = log -1 HEAD
     ls = ls-files
[difftool]
     prompt = false
[mergetool]
     prompt = false
[gc]
    auto = 0

